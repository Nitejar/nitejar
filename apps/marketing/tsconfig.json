{
  "extends": "@nitejar/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts", "next-env.d.ts", ".next/types/**/*.ts"],
  "exclude": ["node_modules", ".next", "out"]
}
